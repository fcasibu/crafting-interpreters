// class 

class hello {
    print_name(name) { print "Hello " + name; }
}

// class instance
var hello_instance = hello();

// get property
hello_instance.print_name("nevz"); // Hello nevz

// set property
hello_instance.print_name = fun(name) {
    print "hello " +  name;
};

hello_instance.print_name("nevz"); // hello nevz

class math {
    add(x) {
        return fun(y) {
            return x + y;
        }
    }

    sub(x) {
        return fun(y) {
            return x - y;
        }
    }

    mul(x) {
        return fun(y) {
            return x * y;
        }
    }
    
    div(x) {
        return fun(y) {
            return x / y;
        }
    }
}

var inst = math();

print inst.add(inst.div(inst.mul(35)(2))(2))(34);

// constructor
class rect {
    init(width, height) {
        this.width = width;
        this.height = height;
    }

    get_area() {
        return this.width * this.height;
    }
}

var rect_inst = rect(160, 80);
print rect_inst.get_area();
